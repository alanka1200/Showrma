<Window x:Class="Showrma.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Showrma"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="27*"/>
            <RowDefinition Height="190*"/>
        </Grid.RowDefinitions>

        <Grid.Background >
            <ImageBrush ImageSource="/Resources/фон.png" Opacity="0.7"></ImageBrush>
        </Grid.Background>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Margin="0,100,0,0" Grid.RowSpan="2"/>
        <TextBlock Background="AliceBlue" Text="Showrma" FontSize="30" Width="142" Height="41"/>
        <Grid Background="LightSlateGray" Grid.Row="0" ZIndex="-2"></Grid>
    </Grid>
</Window>
